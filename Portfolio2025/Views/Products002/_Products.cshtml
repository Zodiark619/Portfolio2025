@using Portfolio2025.Models.Cashier002
@model IEnumerable<Product>


@if (Model != null && Model.Count() > 0)
{
    
    <table class="table table-striped">
        <thead>
            <tr>
                <th>Product Name</th>
                <th>Quantity</th>
            </tr>
        </thead>
        <tbody>
            @foreach (var i in Model)
            {
                <tr class="product-row" product-id="@i.ProductId">
                    <td>@i.Name</td>
                    <td>@i.Quantity</td>
                </tr>
            }
        </tbody>
    </table>

}
